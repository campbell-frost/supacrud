{
  "name": "supacrud",
  "version": "0.9.2",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "shx rm -rf dist && tsc -b",
    "prepublishOnly": "npm run build"
  },
  "author": "Campbell Frost",
  "license": "MIT",
  "description": "Generate CRUD ops for typescript supabase projects",
  "type": "module",
  "dependencies": {
    "@oclif/core": "^4.0.12",
    "@supabase/supabase-js": "^2.44.4",
    "inquirer": "^10.1.0",
    "typescript": "^5.5.3"
  },
  "bin": {
    "supacrud": "bin/run.js"
  },
  "oclif": {
    "bin": "supacrud",
    "commands": {
      "strategy": "single",
      "target": "./dist/supacrud.js"
    },
    "dirname": "supacrud",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ]
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^18.19.41",
    "ts-node": "^10.9.2", 
    "shx": "^0.3.3"
  }
}